{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/flowtype",
    "prettier/react"
  ],

  "env": {
    "browser": true,
    "jest": true
  },

  "rules": {
    "react/require-default-props": 1,
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }]
  }
}
